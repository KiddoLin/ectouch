{
    "name": "ectouch/ectouch",
    "description": "ECTouch E-Commerce Platform for PHP.",
    "keywords": ["ectouch", "b2c", "o2o", "shop", "mall", "wechat"],
    "license": "GPL-2.0",
    "type": "project",
    "homepage": "http://ectouch.cn/",
    "require": {
        "php": ">=7.2.0",
        "topthink/framework": "5.1.*",
        "topthink/think-captcha": "^2.0",
        "topthink/think-helper": "^1.0",
        "topthink/think-image": "^1.0",
        "topthink/think-migration": "^2.0",
        "topthink/think-queue": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app/"
        }
    },
    "extra": {
        "think-path": "vendor/topthink/framework"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
