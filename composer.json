{
    "name": "ectouch/ectouch",
    "description": "ECTouch E-Commerce Platform for PHP.",
    "keywords": ["ectouch", "B2C", "o2o", "shop", "mall", "wechat"],
    "homepage": "https://www.ectouch.cn/",
    "type": "project",
    "license": "GPL-3.0",
    "support": {
        "issues": "https://github.com/ectouch/ectouch/issues?state=open",
        "forum": "https://www.ectouch.cn/forum/",
        "wiki": "https://www.ectouch.cn/wiki/",
        "source": "https://github.com/ectouch/ectouch"
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1.3",
        "aliyuncs/oss-sdk-php": "^2.2",
        "endroid/qr-code": "^3.2",
        "jellybool/translug": "^2.0",
        "jenssegers/agent": "^2.6",
        "overtrue/easy-sms": "^1.0",
        "overtrue/pinyin": "^3.0",
        "overtrue/socialite": "^2.0",
        "overtrue/wechat": "^4.0",
        "phpoffice/phpspreadsheet": "^1.2",
        "riverslei/payment": "^4.1",
        "sentry/sentry": "^1.8",
        "workerman/phpsocket.io": "^1.1",
        "yiisoft/yii2": "~2.0.14",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-httpclient": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0"
    },
    "require-dev": {
        "bizley/migration": "^2.2",
        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1",
        "codeception/specify": "~0.4.3",
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app/"
        },
        "files": [
            "config/constant.php",
            "app/support/helpers.php"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "scripts": {
        "post-install-cmd": [
            "yii\\composer\\Installer::postInstall"
        ],
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject",
            "yii\\composer\\Installer::postInstall"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "storage": "0777",
                    "public/assets": "0777",
                    "artisan": "0755"
                }
            ]
        },
        "yii\\composer\\Installer::postInstall": {
            "generateCookieValidationKey": [
                "config/config.php"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
